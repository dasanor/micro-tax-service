{
  "services": {
    "name": "tax",
    "version": "v1",
    "style": "RPC",
    "uris": {
      "product.list": "catalog:product.list"
    }
  },
  "schemas": {
    "cartTaxes": "${servicePath}/operations/schemas/cartTaxesSchema",
    "infoTax": "${servicePath}/operations/schemas/infoTaxSchema",
    "createTax": "${servicePath}/operations/schemas/createTaxSchema",
    "updateTax": "${servicePath}/operations/schemas/updateTaxSchema",
    "removeTax": "${servicePath}/operations/schemas/removeTaxSchema"
  },
  "models": {
    "Tax": "${servicePath}/models/taxModel"
  },
  "taxes": {
    "defaultCode": "default",
    "classes": {
      "default": "./classes/default",
      "ca-on-pm": "./classes/ca-on-pm"
    }
  },
  "bus": {
    "engines": {
      "local": {
        "handler": "./local"
      },
      "amqp": {
        "handler": "./amqp",
        "url": "amqp://bus"
      }
    },
    "channels": {
      "taxes": {
        "name": "TAXES",
        "engine": "amqp"
      }
    }
  },
  "aggregateItems": true
}
